<div class="main">
  <div class="inner">
    <div class="form__wrapper">
      <h2 class="page-heading">新規プロトタイプ投稿</h2>
       
<%= render 'form', prototype: @prototype, local: true do |f| %>
  <div>
    <%= f.label :title, "タイトル" %>
    <%= f.text_field :title %>
  </div>

  <div>
    <%= f.label :catch_copy, "キャッチコピー" %>
    <%= f.text_area :catch_copy %>
  </div>

  <div>
    <%= f.label :concept, "コンセプト" %>
    <%= f.text_area :concept %>
  </div>

  <div>
    <%= f.label :image, "画像" %>
    <%= f.file_field :image %>
  </div>

  <%= f.submit "投稿する" %>
<% end %>

    </div>
  </div>
</div>